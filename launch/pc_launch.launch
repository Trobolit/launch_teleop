<launch>
 
  <!-- change ip -->
   <env name="ROS_IP" value="10.9.0.2"/>
   <env name="ROS_MASTER_URI" value="http://10.9.0.3:11311"/>

  <!-- joy node -->
   <node respawn="true" pkg="joy" type="joy_node" name="joy">
   <param name="deadzone" value="0.05"/>
   <param name="autorepeat_rate" value="7"/>
   <param name="coalesce_interval" value="0.1"/>
  </node>

</launch>

